{
  "name": "api-aee",
  "version": "0.0.1",
  "description": "AEE project REST API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luizhsou1/aee.git"
  },
  "author": "Luiz Henrique de Souza <luizhsou1@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/luizhsou1/aee/issues"
  },
  "homepage": "https://github.com/luizhsou1/aee#readme",
  "scripts": {
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "typeorm": "./node_modules/.bin/ts-node --transpile-only ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:create": "typeorm migration:create -n",
    "migration:up": "npm run typeorm -- migration:run",
    "migration:down": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "merge-deep": "^3.0.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "^17.0.14",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}